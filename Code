def read(fastafile):
    fastaseq = open(fastafile, 'r')
    originalseq = ""
    for line in fastaseq:
        if line[0] != '>':
            originalseq = originalseq + line.strip()

                
        

def complementary(sequence):
    read(sequence)
    bckwrdseq = sequence[::-1]
    complementaryseq = ""
    for i in bckwrdseq:
        if i == '3':
            complementaryseq.append("5'")
        elif i =='5':
            complementaryseq.append("3' ")
        elif i == 'T':
            complementaryseq.append('A')
        elif i == 'A':
            complementaryseq.append('T')
        elif i == 'G':
            complementaryseq.append('C')
        elif i == 'C':
            complementaryseq.append('G')
        print(complementaryseq)


read("5’ TCAATGTAACGCGCTACCCGGAGCTCTGGGCCCAAATTTCATCCACT 3’")
complementary("5’ TCAATGTAACGCGCTACCCGGAGCTCTGGGCCCAAATTTCATCCACT 3’")

